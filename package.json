{
  "name": "doc-hash",
  "description": "Document hash generator",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start:electron": "electron .",
    "package:electron:windows": "npx @electron/packager --platform=win32 --asar --arch=x64 --icon=dist/favicon.ico --overwrite --out=build .",
    "package:electron:mac": "npx @electron/packager --platform=darwin --asar --arch=arm64 --icon=public/android-chrome-512x512.icns --overwrite --out=build .",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "main": "main.cjs",
  "devDependencies": {
    "@electron/packager": "^18.3.6",
    "@glint/core": "^1.4.0",
    "@types/qunit": "^2.19.10",
    "autoprefixer": "^10.4.19",
    "docx": "^8.5.0",
    "electron": "33.2.1",
    "glint-environment-gxt": "^0.0.4",
    "hash-wasm": "^4.11.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-ember-template-tag": "^2.0.2",
    "qunit": "^2.20.1",
    "qunit-dom": "^3.1.2",
    "qunit-theme-ember": "^0.2.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-babel": "^1.2.0"
  },
  "dependencies": {
    "@lifeart/gxt": "^0.0.42",
    "decorator-transforms": "^2.0.0"
  }
}
